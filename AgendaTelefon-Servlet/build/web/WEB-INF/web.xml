<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>PersonalServlet</servlet-name>
        <servlet-class>PersonalServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdaugareCServlet</servlet-name>
        <servlet-class>AdaugareCServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CautaServlet</servlet-name>
        <servlet-class>CautaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StergeServlet</servlet-name>
        <servlet-class>StergeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModificaServlet</servlet-name>
        <servlet-class>ModificaServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PersonalServlet</servlet-name>
        <url-pattern>/PersonalServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdaugareCServlet</servlet-name>
        <url-pattern>/AdaugareCServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CautaServlet</servlet-name>
        <url-pattern>/CautaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StergeServlet</servlet-name>
        <url-pattern>/StergeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModificaServlet</servlet-name>
        <url-pattern>/ModificaServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
